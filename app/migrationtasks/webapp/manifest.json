{
    "_version": "1.32.0",
    "sap.app": {
        "id": "migrationtasks",
        "type": "application",
        "i18n": {
            "bundleName": "contentviewer.i18n.i18n",
            "supportedLocales": [
                "en"
            ]
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "Migration Tasks",
        "description": "Manage migration task content",
        "dataSources": {
            "mainService": {
                "uri": "odata/v4/config/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "migrationtasks-manage": {
                    "semanticObject": "migrationtasks",
                    "action": "manage",
                    "title": "Migration Tasks",
                    "subTitle": "Configure and Run",
                    "icon": "sap-icon://begin",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sap.btp.cpimigrationtool"
    },
    "sap.platform.cf": {
	   "ui5VersionNumber": "1.138.1"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "css": [
                {
                    "uri": "custom.css"
                }
            ]
        },
        "dependencies": {
            "minUI5Version": "1.130.11",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "contentviewer.i18n.i18n",
                    "supportedLocales": [
                        "en"
                    ]
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "MigrationTasksList",
                    "target": "MigrationTasksList"
                },
                {
                    "pattern": "MigrationTasks({key}):?query:",
                    "name": "MigrationTasksObjectPage",
                    "target": "MigrationTasksObjectPage"
                },
                {
                    "pattern": "MigrationTasks({key})/toMigrationJobs({key2}):?query:",
                    "name": "MigrationJobsObjectPage",
                    "target": "MigrationJobsObjectPage"
                }
            ],
            "targets": {
                "MigrationTasksList": {
                    "type": "Component",
                    "id": "MigrationTasksList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "MigrationTasks",
                            "enhanceI18n": "i18n/i18n.properties",
                            "variantManagement": "None",
                            "navigation": {
                                "MigrationTasks": {
                                    "detail": {
                                        "route": "MigrationTasksObjectPage"
                                    }
                                }
                            },
                            "initialLoad": true,
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "ResponsiveTable",
                                        "selectionMode": "Multi"
                                    }
                                }
                            }
                        }
                    }
                },
                "MigrationTasksObjectPage": {
                    "type": "Component",
                    "id": "MigrationTasksObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "MigrationTasks",
                            "enhanceI18n": "i18n/i18n.properties",
                            "variantManagement": "None",
                            "navigation": {
                                "toMigrationJobs": {
                                    "detail": {
                                        "route": "MigrationJobsObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "toTaskNodes/@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "ResponsiveTable",
                                        "enableFullScreen": true,
                                        "quickVariantSelection": {
                                            "paths": [
                                                {
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Packages"
                                                },
                                                {
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Security"
                                                },
                                                {
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Others"
                                                }
                                            ],
                                            "showCounts": true,
                                            "hideTableTitle": true
                                        }
                                    }
                                },
                                "toMigrationJobs/@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "enableFullScreen": true
                                    }
                                }
                            }
                        }
                    }
                },
                "MigrationJobsObjectPage": {
                    "type": "Component",
                    "id": "MigrationJobsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "MigrationJobs",
                            "variantManagement": "None",
                            "content": {
                                "body": {
                                    "sections": {
                                        "logPage": {
                                            "type": "XMLFragment",
                                            "name": "migrationtasks/ext/logPage",
                                            "title": "Log"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}