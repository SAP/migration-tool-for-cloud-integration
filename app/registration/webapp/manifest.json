{
    "_version": "1.32.0",
    "sap.app": {
        "id": "registration",
        "type": "application",
        "i18n": {
            "bundleName": "contentviewer.i18n.i18n",
            "supportedLocales": [
                "en"
            ]
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "Register Tenants",
        "description": "Register tenants to be used in the migration",
        "dataSources": {
            "mainService": {
                "uri": "odata/v4/registration/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "registration-manage": {
                    "semanticObject": "registration",
                    "action": "manage",
                    "title": "Register Tenants",
                    "subTitle": "Create Connections",
                    "icon": "sap-icon://connected",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sap.btp.cpimigrationtool"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.platform.cf": {
	   "ui5VersionNumber": "1.138.1"
    },
    "sap.ui5": {
        "resources": {
            "css": [
                {
                    "uri": "custom.css"
                }
            ]
        },
        "dependencies": {
            "minUI5Version": "1.130.11",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "contentviewer.i18n.i18n",
                    "supportedLocales": [
                        "en"
                    ]
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "TenantsList",
                    "target": "TenantsList"
                },
                {
                    "pattern": "Tenants({key}):?query:",
                    "name": "TenantsObjectPage",
                    "target": "TenantsObjectPage"
                }
            ],
            "targets": {
                "TenantsList": {
                    "type": "Component",
                    "id": "TenantsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Tenants",
                            "enhanceI18n": "i18n/i18n.properties",
                            "variantManagement": "None",
                            "navigation": {
                                "Tenants": {
                                    "detail": {
                                        "route": "TenantsObjectPage"
                                    }
                                }
                            },
                            "initialLoad": true,
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "ResponsiveTable",
                                        "selectionMode": "Multi"
                                    },
                                    "actions": {
                                        "DataActions": {
                                            "text": "Dev Tools",
                                            "menu": [
                                                "DataFieldForAction::RegistrationService.EntityContainer::exportTenants",
                                                "downloadDatabase",
                                                "downloadTenants"
                                            ],
                                            "enabled": ".extension.registration.ext.customActions.isLocalDeployment"
                                        },
                                        "downloadDatabase": {
                                            "press": ".extension.registration.ext.customActions.downloadDatabase",
                                            "enabled": true,
                                            "text": "Download db.sqlite file"
                                        },
                                        "downloadTenants": {
                                            "press": ".extension.registration.ext.customActions.downloadTenants",
                                            "enabled": true,
                                            "text": "Download Tenants.csv file"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "TenantsObjectPage": {
                    "type": "Component",
                    "id": "TenantsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "Tenants",
                            "enhanceI18n": "i18n/i18n.properties",
                            "variantManagement": "None",
                            "navigation": {},
                            "content": {
                                "header": {
                                    "actions": {
                                        "importSettings": {
                                            "press": ".extension.registration.ext.customActions.setValues",
                                            "text": "Import oAuth Service Key",
                                            "enabled": ".extension.registration.ext.customActions.isEditing",
                                            "visible": true
                                        }
                                    }
                                }
                            },
                            "editableHeaderContent": false
                        }
                    }
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "registration.ext.customActions"
                    },
                    "sap.fe.templates.ObjectPage.ObjectPageController": {
                        "controllerName": "registration.ext.customActions"
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}