{
  "name": "migrationtool",
  "version": "1.3.1",
  "description": "Assistant to support content migration from SAP Integration Services (Neo) to SAP Integration Suite (Multi-cloud)",
  "repository": "https://github.com/SAP/migration-tool-for-cloud-integration",
  "license": "Apache-2.0",
  "private": false,
  "author": {
    "name": "Willem Pardaens",
    "email": "willem.pardaens@sap.com"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "SAP Integration Suite",
    "SAP Cloud Integration",
    "Migration tool"
  ],
  "dependencies": {
    "@cap-js/hana": "^2",
    "@cap-js/postgres": "^2",
    "@cap-js/sqlite": "^2",
    "@sap-cloud-sdk/http-client": "^4.0",
    "@sap/cds": "~9.0",
    "@sap/xssec": "^4",
    "axios": "^1.9",
    "express": "^4",
    "qs": "^6.14",
    "xml2js": "^0.6",
    "yauzl": "^3.2",
    "yazl": "^3.3"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.35",
    "@cap-js/cds-types": "^0.11",
    "@sap/cds-dk": "^9.0",
    "@types/node": "^24.0",
    "@types/qs": "^6.14",
    "@types/xml2js": "^0.4",
    "@types/yauzl": "^2.10",
    "@types/yazl": "^3.3",
    "eslint": "^9.28",
    "rimraf": "^6",
    "tsx": "^4.20",
    "typescript": "^5.8"
  },
  "scripts": {
    "build": "rimraf resources mta_archives && cds build --production",
    "lint": "eslint",
    "start": "cds-serve",
    "undeploy": "cf undeploy migrationtool --delete-services --delete-service-keys --delete-service-brokers",
    "watch": "cds-ts w"
  },
  "cds": {
    "log": {
      "format": "plain"
    },
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": {
          "kind": "hana"
        }
      },
      "[pg-hybrid]": {
        "db": {
          "kind": "postgres",
          "pool": {
            "acquireTimeoutMillis": 3000,
            "max": 10
          },
          "credentials": {
            "hostname": "127.0.0.1",
            "port": "12345",
            "username": "...",
            "password": "...",
            "dbname": "...",
            "sslcert": "-----BEGIN CERTIFICATE-----\n...\n-----END CERTIFICATE-----",
            "sslrootcert": "-----BEGIN CERTIFICATE-----\n...\n-----END CERTIFICATE-----"
          }
        },
        "auth": "dummy"
      },
      "[hana-hybrid]": {
        "db": {
          "kind": "hana"
        },
        "auth": "dummy"
      },
      "[development]": {
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        },
        "auth": "dummy"
      },
      "html5-repo": true
    }
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}