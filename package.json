{
  "name": "migrationtool",
  "version": "1.0.0",
  "description": "Assistant to support content migration from SAP Integration Services (Neo) to SAP Integration Suite (Multi-cloud)",
  "repository": "https://github.com/SAP/migration-tool-for-cloud-integration",
  "license": "Apache-2.0",
  "private": false,
  "author": {
    "name": "Willem Pardaens",
    "email": "willem.pardaens@sap.com"
  },
  "engines": {
    "node": ">=14.5"
  },
  "keywords": [
    "SAP Integration Suite",
    "SAP Cloud Integration",
    "Migration tool"
  ],
  "dependencies": {
    "@sap/cds": "~5.7.3",
    "axios": "^0.24",
    "express": "^4.17.1",
    "sqlite3": "https://github.com/mapbox/node-sqlite3/tarball/master",
    "xml2js": "^0.4.23",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.0"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.95"
  },
  "scripts": {
    "start": "cds run --with-mocks"
  },
  "cds": {
    "requires": {
      "IntegrationContent.sap.hci.api": {
        "kind": "odata",
        "model": "srv/external/IntegrationContent"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "sqlite.db"
        }
      }
    }
  }
}
